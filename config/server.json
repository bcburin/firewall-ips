{
    "host": "127.0.0.1",
    "port": 8000,
    "database": {
        "name": "fw-rules",
        "host": "127.0.0.1",
        "port": 5432,
        "dbms": "postgresql",
        "user": "xxx",
        "password": "xxx",
        "verbose": true
    },
    "aiModule": {
        "training": {
            "cronString": "* * * * *",
            "runOnStart": false,
            "data": {
                "type": "file",
                "path": "$root/data/log.xlsx"
            }
        },
        "persistence": {
            "enable": true,
            "directory": "$root/archive",
            "maxSavedModels": 3,
            "compression": {
                "enable": true,
                "tool": "zip",
                "compressLatestModel": false
            }
        }
    },
    "devMode": true
}
{
    "host": "127.0.0.1",
    "port": 8000,
    "database": {
        "name": "fw-rules",
        "host": "127.0.0.1",
        "port": 5432,
        "dbms": "postgresql",
        "user": "xxx",
        "password": "xxx",
        "verbose": true
    },
    "aiModule": {
        "training": {
            "cronString": "* * * * *",
            "runOnStart": false,
            "data": {
                "type": "directory",
                "path": "$root/data/final_data"
            }
        },
        "staticRuleCreation": {
            "cronString":"0 0 0 ? * SUN *",
            "runOnStart": false,
            "data": {
                "type": "directory",
                "path": "$root/data/rule_creationn"
            }
        },
        "persistence": {
            "directory": "$root/repository",
            "maxSavedModels": 3,
            "loadLatestOnStart": true,
            "compression": {
                "enable": true,
                "tool": "zip"
            }
        }
    },
    "notification": {
        "enable": true,
        "maxQueueSize": 128,
        "cronString": "* * * * *",
        "methods": {
            "email": {
                "subject": "Pacotes suspeitos: regras de firewall criadas [{n}]",
                "templatePath": "$statics/fw-rule-email-template-pt.html.jinja2",
                "smtp": {
                    "server": "127.0.0.1",
                    "port": 1025
                },
                "sender": {
                    "email": "test.user@mailpit.com",
                    "password": "password"
                },
                "mailingList": ["user1@mailpit.com", "user2@mailpit.com"]
            }
        }
    },
    "authentication": {
        "token": {
            "key": "09d25e094faa6ca2556c818166b7a9563b93f7099f6f0f4caa6cf63b88e8d3e7",
            "algorithm": "HS256",
            "expirationTimeInMinutes": 30
        },
        "login": {
            "method": "JWTAuthService",
            "maxLoginTries": 5,
            "notifyOnMaxTries": true
        }
    },
    "devMode": true
}